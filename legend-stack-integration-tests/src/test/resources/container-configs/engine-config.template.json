{
  "deployment": {
    "mode": "TEST_IGNORE_FUNCTION_MATCH"
  },
  "logging": {
    "level": "info",
    "loggers": {
      "root": {
        "level": "info"
      },
      "org.pac4j": {
        "level": "debug"
      }
    },
    "appenders": [
      {
        "type": "console",
        "logFormat": "%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p [%thread] %c - %m%n"
      }
    ]
  },
  "pac4j": {
    "callbackPrefix": "",
    "mongoUri": "__MONGO_URI__",
    "mongoDb": "legend",
    "bypassPaths": [
      "/api/server/v1/info"
    ],
    "clients": [
      {
        "org.finos.legend.server.pac4j.gitlab.GitlabClient": {
          "name": "gitlab",
          "clientId": "__GITLAB_CLIENTID__",
          "secret": "__GITLAB_CLIENTID_SECRET__",
          "discoveryUri": "https://gitlab.com/.well-known/openid-configuration",
          "scope": "openid profile api"
        }
      },
      {
        "org.pac4j.core.client.direct.AnonymousClient": {
        }
      }
    ],
    "mongoSession": {
      "enabled": true,
      "collection": "userSessions"
    }
  },
  "opentracing": {
    "elastic": "",
    "zipkin": "",
    "uri": "",
    "authenticator": {
      "principal": "",
      "keytab": ""
    }
  },
  "swagger": {
    "title": "Legend Execution REST Services",
    "resourcePackage": "org.finos.legend",
    "uriPrefix": "/exec/api"
  },
  "server": {
    "type": "simple",
    "applicationContextPath": "/exec",
    "adminContextPath": "/exec/admin",
    "connector": {
      "maxRequestHeaderSize": "32KiB",
      "type": "http",
      "port": __LEGEND_ENGINE_PORT__
    },
    "requestLog": {
      "appenders": [
      ]
    }
  },
  "metadataserver": {
    "pure": {
      "host": "127.0.0.1",
      "port": 8090
    }
  },
  "vaults": [
  ],
  "relationalexecution": {
    "tempPath": "/tmp/",
    "temporarytestdb": {
      "port": 9092
    }
  }
}